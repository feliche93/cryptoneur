{
  "kind": "collectionType",
  "collectionName": "grant_use_cases",
  "info": {
    "singularName": "grant-use-case",
    "pluralName": "grant-use-cases",
    "displayName": "Grant Use Case"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "string"
    },
    "color": {
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "grants": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::grant.grant",
      "inversedBy": "grantUseCases"
    }
  }
}
