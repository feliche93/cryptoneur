{
  "name": "src",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm concurrently \"contentlayer dev\" \"next dev\"",
    "build": "pnpm contentlayer build && pnpm next build",
    "preview": "pnpm next build && pnpm next start",
    "start": "pnpm next start",
    "lint": "prettier . --check && pnpm eslint",
    "lint:fix": "prettier . --write && eslint --fix",
    "lint:format": "prettier . --write",
    "type-check": "tsc --pretty --noEmit",
    "generate": "drizzle-kit generate:pg --config=drizzle.config.ts",
    "update-pg": "drizzle-kit up:pg --config=drizzle.config.ts",
    "check": "drizzle-kit check:pg --config=drizzle.config.ts",
    "drop": "drizzle-kit drop --config=drizzle.config.ts",
    "generate-content": "pnpm contentlayer build",
    "generate-posthog-sdk": "npx openapi-typescript-codegen --input ./openapi-specs/posthog.yaml --output ./src/lib/posthog-typescript-sdk --name client --useOptions --client fetch --useUnionTypes",
    "generate-papierkram-sdk": "npx openapi-typescript-codegen --input https://felixvemmer.papierkram.de/api/v1/api-docs/api/v1/swagger.json --output ./src/lib/papierkram-typescript-sdk --name client --useOptions --client fetch --useUnionTypes",
    "clean": "rm -rf pnpm-lock.yaml node_modules .next"
  },
  "dependencies": {
    "@calcom/embed-react": "^1.3.2",
    "@clerk/nextjs": "^4.29.9",
    "@google-analytics/data": "^3.3.0",
    "@hookform/resolvers": "^3.3.4",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@react-icons/all-files": "^4.1.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@tanstack/match-sorter-utils": "^8.11.8",
    "@tanstack/react-query": "^4.36.1",
    "@tanstack/react-table": "^8.13.2",
    "@tiptap/pm": "2.2.4",
    "@tiptap/react": "2.2.4",
    "@tiptap/starter-kit": "2.2.4",
    "@vercel/analytics": "^1.2.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.10",
    "lodash": "^4.17.21",
    "lucide-react": "^0.358.0",
    "next": "14.2.0-canary.28",
    "next-intl": "^3.9.5",
    "next-share": "^0.27.0",
    "next-themes": "^0.3.0",
    "openai": "^3.2.1",
    "posthog-js": "^1.116.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.51.1",
    "react-icons": "^4.8.0",
    "react-select": "^5.8.0",
    "schema-dts": "^1.1.2",
    "server-only": "^0.0.1",
    "sharp": "^0.32.6",
    "slugify": "^1.6.6",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@hookform/devtools": "^4.3.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/lodash": "^4.17.0",
    "@types/node": "20.11.28",
    "@types/react": "18.2.67",
    "@types/react-dom": "18.2.22",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "autoprefixer": "^10.4.18",
    "encoding": "^0.1.13",
    "eslint": "8.57.0",
    "eslint-config-next": "14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-mdx": "^3.1.5",
    "postcss": "^8.4.36",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "tailwindcss": "^3.4.1",
    "typescript": "5.4.2"
  }
}
