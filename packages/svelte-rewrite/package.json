{
	"name": "nocodescraper",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"generate": "drizzle-kit generate:pg --config=drizzle.config.ts",
		"drop": "drizzle-kit drop --config=drizzle.config.ts",
		"push": "drizzle-kit push:pg --config=drizzle.config.ts"
	},
	"devDependencies": {
		"@melt-ui/pp": "^0.1.2",
		"@playwright/test": "^1.37.1",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.23.0",
		"@types/pg": "^8.10.2",
		"@typescript-eslint/eslint-plugin": "^6.4.1",
		"@typescript-eslint/parser": "^6.4.1",
		"autoprefixer": "^10.4.15",
		"daisyui": "^3.6.3",
		"drizzle-kit": "^0.19.13",
		"eslint": "^8.47.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.33.0",
		"postcss": "^8.4.28",
		"prettier": "^3.0.2",
		"prettier-plugin-svelte": "^3.0.3",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.0",
		"svelte-sequential-preprocessor": "^2.0.1",
		"svelte-share-buttons-component": "^1.5.5",
		"sveltekit-superforms": "^1.6.1",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.4.9",
		"vitest": "^0.34.3",
		"zod": "^3.22.2"
	},
	"type": "module",
	"dependencies": {
		"@lucia-auth/adapter-postgresql": "^2.0.1",
		"@lucia-auth/oauth": "^3.0.0",
		"@melt-ui/svelte": "^0.34.6",
		"@neondatabase/serverless": "^0.5.7",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.0.0",
		"dotenv": "^16.3.1",
		"drizzle-orm": "^0.28.5",
		"langchain": "^0.0.125",
		"lucia": "^2.4.0",
		"lucia-auth": "^1.8.0",
		"lucide-svelte": "^0.265.0",
		"pg": "^8.11.3",
		"postgres": "^3.3.5",
		"posthog-js": "^1.77.2",
		"tailwind-merge": "^1.14.0",
		"tailwind-variants": "^0.1.13",
		"tailwindcss-animate": "^1.0.6"
	}
}